{
    "packageDirectories": [
        {
            "path": "./src-env-specific-pre",
            "package": "src-env-specific-pre",
            "versionNumber": "1.0.0.0",
            "ignoreOnStage": ["prepare", "validate", "quickbuild", "build"]
        },
        {
            "path": "./src-env-specific-alias-pre",
            "package": "src-env-specific-alias-pre",
            "versionNumber": "1.0.0.0",
            "aliasfy": true,
            "ignoreOnStage": ["prepare", "validate", "quickbuild", "build"]
        },
        {
            "path": "./src/core",
            "package": "core",
            "versionNumber": "1.0.0.0",
            "versionNumber": "1.0.0.0",
            "default": false,
            "type": "source",
            "ignoreOnStage": ["prepare", "validate", "quickbuild", "build"]
        },
        {
            "path": "./src/utils/cqrs",
            "package": "cqrs-service",
            "versionName": "version 1.0.0",
            "versionNumber": "1.0.0.0",
            "default": true,
            "skipCoverageValidation": false,
            "skipTesting": false,
            "isOptimizedDeployment": true,
            "type": "source",
            "alwaysDeploy": true,
            "preDeploymentScript": "scripts/predeploy.sh",
            "postDeploymentScript": "scripts/postdeploy.sh"
        },
        {
            "path": "./src-ui",
            "package": "src-ui",
            "versionNumber": "1.1.0.0",
            "default": false,
            "ignoreOnStage": ["prepare", "validate", "quickbuild", "build"]
        },
        {
            "path": "./src-access-management",
            "package": "src-access-management",
            "versionNumber": "1.0.0.0",
            "default": false,
            "ignoreOnStage": ["prepare", "validate", "quickbuild", "build"]
        },
        {
            "path": "./src-env-specific-alias-post",
            "package": "src-env-specific-alias-post",
            "versionNumber": "1.0.0.0",
            "aliasfy": true,
            "alwaysDeploy": true,
            "ignoreOnStage": ["prepare", "validate", "quickbuild", "build"]
        },
        {
            "path": "./src-temp",
            "package": "src-temp",
            "versionName": "version 1.0.0",
            "versionNumber": "1.0.0.0",
            "default": false,
            "ignoreOnStage": ["prepare", "validate", "quickbuild", "build"]
        }
    ],
    "namespace": "",
    "sfdcLoginUrl": "https://login.salesforce.com",
    "sourceApiVersion": "52.0",
    "packageAliases": {},
    "plugins": {}
}
